<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">

<!--    solo ejecutara el groups-->
    <groups>
        <run>
            <include name="ErrorHandling"/>
        </run>
    </groups>

    <test verbose="2" preserve-order="true" name="test de error en login">
        <classes>
            <class name="org.example.seleniumTest.clientLoginErrorPageFactory"/>
        </classes>
    </test>

    <test verbose="2" preserve-order="true" name="test de error en register">
        <classes>
            <class name="org.example.seleniumTest.clientRegisterErrorPageFactory"/>
        </classes>
    </test>
</suite>